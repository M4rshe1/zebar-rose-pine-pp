{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.0.0/resources/zpack-schema.json",
  "name": "zebar-rose-pine-pp",
  "version": "1.1.3",
  "description": "A fully featured topbar derived from the Ros√© Pine theme",
  "tags": [
    "topbar",
    "glazewm",
    "rose-pine"
  ],
  "previewImages": [
    "./resources/preview-image-1.png",
    "./resources/preview-image-2.png"
  ],
  "repositoryUrl": "https://github.com/m4rshe1/zebar-rose-pine-pp",
  "widgets": [
    {
      "name": "glazewm",
      "htmlPath": "./dist/glazewm/glazewm.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": true,
      "transparent": true,
      "includeFiles": [
        "dist/glazewm/*.html",
        "dist/glazewm/*.css",
        "dist/glazewm/*.js"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "48px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    },
    {
      "name": "komorebi",
      "htmlPath": "./dist/komorebi/komorebi.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": true,
      "transparent": true,
      "includeFiles": [
        "dist/komorebi/*.html",
        "dist/komorebi/*.css",
        "dist/komorebi/*.js"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "48px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    },
    {
      "name": "vanilla",
      "htmlPath": "./dist/vanilla/vanilla.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": true,
      "transparent": true,
      "includeFiles": [
        "dist/vanilla/*.html",
        "dist/vanilla/*.css",
        "dist/vanilla/*.js"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "48px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
